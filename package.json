{
  "name": "@minsk/util",
  "version": "1.0.0",
  "description": "js util like get type from js etc.",
  "module": "./index.mjs",
  "scripts": {
    "dev": "rollup -cw",
    "test": "mocha",
    "build": "node ./scripts/build.js",
    "build:types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/util.mjs",
      "require": "./index.js"
    },
    "./dist/*": "./dist/*",
    "./types/*": [
      "./types/*",
      "./types/*.d.ts"
    ]
  },
  "keywords": [
    "js",
    "util",
    "typeof",
    "js type"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.25",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  }
}
